{
    "name": "russsiq/bixbite",
    "description": "Система для оперативного развертывания простейших сайтов на shared хостингах на базе Laravel.",
    "keywords": ["bixbite", "cms", "laravel"],
    "homepage": "https://github.com/russsiq/bixbite",
    "type": "project",

    "license": "MIT",
    "authors": [{
        "name": "Рустам Гимранов",
        "email": "rustam_gimranov@mail.ru",
        "homepage": "https://github.com/russsiq",
        "role": "Developer"
    }],
    "repositories": [{
        "type": "vcs",
        "url": "https://github.com/russsiq/bixbite"
    }],
    "require": {
        "php": ">=7.3.0",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^6.5",
        "laravel/framework": "^7.0",
        "laravel/tinker": "^2.0",
        "russsiq/laravel-assistant": "dev-master",
        "russsiq/laravel-dom-manipulator": "dev-master",
        "russsiq/laravel-env-manager": "^0.2",
        "russsiq/laravel-grecaptcha": "^0.1",
        "russsiq/laravel-zipper": "^0.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.1",
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.9.1",
        "laravel/ui": "^2.0",
        "mockery/mockery": "^1.4.0",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^9.1"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "test": [
            "@php artisan config:clear",
            "vendor/bin/phpunit"
        ],
        "test-win7": [
            "@php artisan config:clear",
            "vendor\\bin\\phpunit"
        ],
        "testdox": [
            "@php artisan config:clear",
            "vendor\\bin\\phpunit --testdox-html tests/testdox.html"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --no-ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "prestissimo": {
            "maxConnections": 10,
            "minConnections": 3,
            "pipeline": true
        },
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
