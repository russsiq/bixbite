{
    "name": "russsiq/bixbite",
    "description": "BixBite – проект на Laravel для создания и управления контентом.",
    "keywords": [
        "bixbite",
        "cms",
        "laravel"
    ],
    "homepage": "https://github.com/russsiq/bixbite",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Рустам Гимранов",
            "email": "rustam_gimranov@mail.ru",
            "homepage": "https://github.com/russsiq",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "8.1.*",
        "ext-bcmath": "*",
        "ext-ctype": "*",
        "ext-dom": "*",
        "ext-fileinfo": "*",
        "ext-gd": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "ext-tokenizer": "*",
        "ext-xml": "*",
        "ext-zip": "*",
        "ext-zlib": "*",
        "fruitcake/laravel-cors": "^3.0.0",
        "guzzlehttp/guzzle": "^7.4.2",
        "laravel/fortify": "^1.12.0",
        "laravel/framework": "^9.11.0",
        "laravel/sanctum": "^2.15.1",
        "laravel/tinker": "^2.7.2",
        "russsiq/laravel-assistant": "^0.5.0",
        "russsiq/laravel-dom-manipulator": "^0.2.0",
        "russsiq/laravel-env-manager": "^0.6.0",
        "russsiq/laravel-grecaptcha": "^0.3.0",
        "russsiq/laravel-zipper": "^0.3.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6.7",
        "brianium/paratest": "^6.4.4",
        "doctrine/dbal": "^3.3.5",
        "fakerphp/faker": "^1.19.0",
        "mockery/mockery": "^1.5.0",
        "nunomaduro/collision": "^6.1.0",
        "phpunit/phpunit": "^9.5.20",
        "spatie/laravel-ignition": "^1.2.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "testdox": [
            "@php artisan config:clear",
            "vendor/bin/phpunit --testdox-html tests/testdox.html",
            "@php artisan optimize"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan optimize"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "prestissimo": {
            "maxConnections": 10,
            "minConnections": 3,
            "pipeline": true
        },
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
